{
  "host": "localhost",
  "port": 3030,
  "blockchain": {
    "provider": "ws://localhost:8545"
  },
  "storage": {
    "contractAddress": "0x83C5541A6c8D2dBAD642f385d8d06Ca9B6C731ee",
    "events": ["CapacitySet", "MaximumDurationSet", "PriceSet"],
    "eventsEmitter": {
      "confirmations": 0,
      "startingBlock": "genesis"
    }
  },
  "rns-owner": {
    "events": ["Transfer", "ExpirationChanged"],
    "eventsEmitter": {
      "confirmations": 0,
      "startingBlock": "genesis"
    },
    "newBlockEmitter": {
      "polling": false
    }
  },
  "rns-reverse": {
    "events": ["NameChanged"],
    "eventsEmitter": {
      "confirmations": 0,
      "startingBlock": "genesis"
    },
    "newBlockEmitter": {
      "polling": false
    }
  },
  "rns-placement": {
    "events": ["UpdatePlacement"],
    "eventsEmitter": {
      "confirmations": 0,
      "startingBlock": "genesis"
    },
    "newBlockEmitter": {
      "polling": false
    }
  },
  "db": "postgres://dev:dev@localhost/rif_marketplace_cache",
  "log": {
    "level": "debug",
    "path": "./logs"
  }
}
